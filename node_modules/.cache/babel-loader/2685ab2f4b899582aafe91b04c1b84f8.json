tle: '商家管理'\n        }]\n      }, {\n        icon: 'el-icon-star-on',\n        index: 'charts',\n        title: 'schart图表'\n      }, {\n        icon: 'el-icon-rank',\n        index: 'drag',\n        title: '拖拽列表'\n      }, {\n        icon: 'el-icon-warning',\n        index: 'permission',\n        title: '权限测试'\n      }, {\n        icon: 'el-icon-error',\n        index: '404页面',\n        title: '404页面'\n      }]\n    };\n  },\n  computed: {\n    // 设置侧边栏默认栏\n    onRoutes: function onRoutes() {\n      return this.$route.path.replace('/', '');\n    }\n  },\n  created: function created() {\n    var _this = this;\n\n    // 通过 Event Bus 进行组件间通信，来折叠侧边栏\n    bus.$on('collapse', function (msg) {\n      _this.collapse = msg;\n    });\n  }\n};",{"version":3,"sources":["Sidebar.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwCA,OAAA,GAAA,MAAA,kBAAA;AACA,eAAA;AACA,MADA,kBACA;AACA,WAAA;AACA,gBAAA,KADA;AAEA,aAAA,CACA;AAAA,cAAA,iBAAA;AAAA,eAAA,WAAA;AAAA,eAAA;AAAA,OADA,EAEA;AAAA,cAAA,iBAAA;AAAA,eAAA,OAAA;AAAA,eAAA;AAAA,OAFA,EAGA;AAAA,cAAA,iBAAA;AAAA,eAAA,MAAA;AAAA,eAAA;AAAA,OAHA,EAIA;AAAA,cAAA,cAAA;AAAA,eAAA,GAAA;AAAA,eAAA,MAAA;AAAA,cAAA,CACA;AAAA,iBAAA,MAAA;AAAA,iBAAA;AAAA,SADA,EAEA;AAAA,iBAAA,KAAA;AAAA,iBAAA,KAAA;AAAA,gBAAA,CACA;AAAA,mBAAA,QAAA;AAAA,mBAAA;AAAA,WADA,EAEA;AAAA,mBAAA,UAAA;AAAA,mBAAA;AAAA,WAFA;AAAA,SAFA,EAOA;AAAA,iBAAA,QAAA;AAAA,iBAAA;AAAA,SAPA;AAAA,OAJA,EAaA;AAAA,cAAA,iBAAA;AAAA,eAAA,CAAA;AAAA,eAAA,MAAA;AAAA,cAAA,CACA;AAAA,iBAAA,UAAA;AAAA,iBAAA;AAAA,SADA,EAEA;AAAA,iBAAA,UAAA;AAAA,iBAAA;AAAA,SAFA;AAAA,OAbA,EAiBA;AAAA,cAAA,iBAAA;AAAA,eAAA,QAAA;AAAA,eAAA;AAAA,OAjBA,EAkBA;AAAA,cAAA,cAAA;AAAA,eAAA,MAAA;AAAA,eAAA;AAAA,OAlBA,EAmBA;AAAA,cAAA,iBAAA;AAAA,eAAA,YAAA;AAAA,eAAA;AAAA,OAnBA,EAoBA;AAAA,cAAA,eAAA;AAAA,eAAA,OAAA;AAAA,eAAA;AAAA,OApBA;AAFA,KAAA;AAyBA,GA3BA;AA4BA,YAAA;AACA;AACA,YAFA,sBAEA;AACA,aAAA,KAAA,MAAA,CAAA,IAAA,CAAA,OAAA,CAAA,GAAA,EAAA,EAAA,CAAA;AACA;AAJA,GA5BA;AAkCA,SAlCA,qBAkCA;AAAA;;AACA;AACA,QAAA,GAAA,CAAA,UAAA,EAAA,eAAA;AACA,YAAA,QAAA,GAAA,GAAA;AACA,KAFA;AAGA;AAvCA,CAAA","sourcesContent":["<template>\n    <div class=\"sidebar\">\n        <!-- 创建一级菜单 -->\n        <el-menu class=\"sidebar-el-menu\" :default-active=\"onRoutes\" :collapse=\"collapse\" background-color=\"#324157\" text-color=\"#bfcbd9\" active-text-color=\"#20a0ff\" uniquer-opened router>\n            <!-- 遍历一级菜单 -->\n            <template v-for=\"item in items\">\n                <!-- 1 如果是有二级菜单：通过subs判断 -->\n                <template v-if=\"item.subs\">\n                    <!-- 一级菜单创建下级 -->\n                    <el-submenu :index=\"item.index\" :key=\"item.index\">\n                        <!-- 1.1 将一级菜单写入页面 -->\n                        <template slot=\"title\">\n                            <i :class=\"item.icon\"></i><span slot=\"title\">{{ item.title }}</span>\n                        </template>\n                        <!-- 1.2 遍历二级菜单 -->\n                        <template v-for=\"subItem in item.subs\">\n                            <!-- 1.2.1 如果是有二级菜单：通过subs判断 -->\n                            <el-submenu v-if=\"subItem.subs\" :index=\"subItem.index\" :key=\"subItem.index\">\n                                <template slot=\"title\">{{ subItem.title }}</template>\n                                <!-- 1.2.1.1 遍历三级菜单,并写入页面 -->\n                                <el-menu-item v-for=\"(threeItem,i) in subItem.subs\" :key=\"i\" :index=\"threeItem.index\">\n                                    {{ threeItem.title }}\n                                </el-menu-item>\n                            </el-submenu>\n                            <!-- 1.2. 将二级菜单写入页面 -->\n                            <el-menu-item v-else :index=\"subItem.index\" :key=\"subItem.index\">{{ subItem.title }}</el-menu-item>\n                        </template>\n                    </el-submenu>\n                </template>\n                <!-- 2 将一级菜单写入页面 -->\n                <template v-else>\n                    <el-menu-item :index=\"item.index\" :key=\"item.index\">\n                        <i :class=\"item.icon\"></i><span slot=\"title\">{{ item.title }}</span>\n                    </el-menu-item>\n                </template>\n            </template>\n        </el-menu>\n    </div>\n</template>\n<script>\nimport bus from '../common/bus.js'\nexport default {\n    data(){\n        return {\n            collapse: false,\n            items: [\n                { icon: 'el-icon-setting', index: 'dashboard', title: '系统首页' },\n                { icon: 'el-icon-tickets', index: 'table', title: '项目管理'},\n                { icon: 'el-icon-message', index: 'tabs', title: 'tab选项卡'},\n                { icon: 'el-icon-date', index: '3', title: '表单相关', subs: [\n                            { index: 'form', title: '基本表单'},\n                            { index: '3-2', title: '编辑器', subs: [\n                                    { index: 'editor', title: '富文本编辑器'},\n                                    { index: 'markdown',title: 'markdown编辑器'},\n                                ]\n                            },\n                            { index: 'upload', title: '文件上传'}\n                        ]},\n                { icon: 'el-icon-message', index: 2, title: '数据管理',subs: [\n                    { index: 'userlist', title: '用户列表'},\n                    { index: 'shoplist', title: '商家管理'},\n                ]},\n                { icon: 'el-icon-star-on', index: 'charts', title: 'schart图表'},\n                { icon: 'el-icon-rank', index: 'drag', title: '拖拽列表'},\n                { icon: 'el-icon-warning', index: 'permission', title: '权限测试'},\n                { icon: 'el-icon-error', index: '404页面',title: '404页面'}\n            ]\n        }\n    },\n    computed:{\n        // 设置侧边栏默认栏\n        onRoutes(){\n            return this.$route.path.replace('/','')\n        }\n    },\n    created(){\n        // 通过 Event Bus 进行组件间通信，来折叠侧边栏\n        bus.$on('collapse' , msg => {\n            this.collapse = msg\n        })\n    }\n}\n</script>\n<style scoped>\n    .sidebar{\n        display: block;\n        position: absolute;\n        left: 0;\n        top: 70px;\n        bottom:0;\n        overflow-y: scroll;\n    }\n    .sidebar::-webkit-scrollbar{\n        width: 0;\n    }\n    .sidebar-el-menu:not(.el-menu--collapse){\n        width: 250px;\n    }\n    .sidebar > ul {\n        height:100%;\n    }\n</style>\n"],"sourceRoot":"src/components/common"}]}